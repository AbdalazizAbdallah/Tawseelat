<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/client_main_graph"
    app:startDestination="@id/clientMainFragment">

    <fragment
        android:id="@+id/clientMainFragment"
        android:name="com.abdalazizabdallah.tawseelat.view.ClientMainFragment"
        android:label="fragment_client_main"
        tools:layout="@layout/fragment_client_main">
        <action
            android:id="@+id/action_clientMainFragment_to_search_fragment"
            app:destination="@id/searchFragment" />
    </fragment>
    <fragment
        android:id="@+id/searchFragment"
        android:name="com.abdalazizabdallah.tawseelat.view.SearchFragment"
        android:label="fragment_search_fragment"
        tools:layout="@layout/fragment_search_fragment" />
</navigation>